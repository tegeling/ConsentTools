    <formulas>
        <name>calcCPAddressName</name>
        <dataType>String</dataType>
        <expression
    >{!$Record.FirstName} + &apos; &apos; + {!$Record.LastName} + &apos; - Mailing Address&apos;</expression>
    </formulas>
    <formulas>
        <name>calcCPCEmailName</name>
        <dataType>String</dataType>
        <expression
    >{!$Record.FirstName} + &apos; &apos; + {!$Record.LastName} + &apos; - &apos; + {!$Record.Email} + &apos; - &apos; + {!For_each_DUP.Name}</expression>
    </formulas>
    <formulas>
        <name>calcCPCMailingAddressName</name>
        <dataType>String</dataType>
        <expression
    >{!$Record.FirstName} + &apos; &apos; + {!$Record.LastName} + &apos; - MailingAddress - &apos; + {!For_each_DUP.Name}</expression>
    </formulas>
    <formulas>
        <name>calcCPCMobileName</name>
        <dataType>String</dataType>
        <expression
    >{!$Record.FirstName} + &apos; &apos; + {!$Record.LastName} + &apos; - &apos; + {!$Record.MobilePhone} + &apos; - &apos; + {!For_each_DUP.Name}</expression>
    </formulas>
    <formulas>
        <name>calcCPCPhoneName</name>
        <dataType>String</dataType>
        <expression
    >{!$Record.FirstName} + &apos; &apos; + {!$Record.LastName} + &apos; - &apos; + {!$Record.Phone} + &apos; - &apos; + {!For_each_DUP.Name}</expression>
    </formulas>
    <formulas>
        <name>calcCPTCEmailName</name>
        <dataType>String</dataType>
        <expression
    >&apos;Email - &apos; + {!$Record.FirstName} + &apos; &apos; + {!$Record.LastName} + &apos; - &apos; + {!For_each_DUP_CPTC_Email.Name}</expression>
    </formulas>
    <formulas>
        <name>calcCPTCMailingAddress</name>
        <dataType>String</dataType>
        <expression
    >&apos;Mailing Address - &apos; + {!$Record.FirstName} + &apos; &apos; + {!$Record.LastName} + &apos; - &apos; + {!For_each_DUP_CPTC_Mailing_Address.Name}</expression>
    </formulas>
    <formulas>
        <name>calcCPTCMobileName</name>
        <dataType>String</dataType>
        <expression
    >&apos;Phone - &apos; + {!$Record.FirstName} + &apos; &apos; + {!$Record.LastName} + &apos; - &apos; + {!For_each_DUP_CPTC_Mobile.Name}</expression>
    </formulas>
    <formulas>
        <name>calcCPTCPhoneName</name>
        <dataType>String</dataType>
        <expression
    >&apos;Phone - &apos; + {!$Record.FirstName} + &apos; &apos; + {!$Record.LastName} + &apos; - &apos; + {!For_each_DUP_CPTC_Phone.Name}</expression>
    </formulas>
    <formulas>
        <name>calcEmailConsentStatus</name>
        <dataType>String</dataType>
        <expression
    >if ({!$Record.HasOptedOutOfEmail}, &apos;OptOut&apos;, &apos;OptIn&apos;)</expression>
    </formulas>
